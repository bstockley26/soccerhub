@model IEnumerable<Recruit>

<h2>Recruits</h2>
<div style="display: block">
    <a href="/Recruit/InsertRecruit/">Create a New Recruit</a>
</div>
<table class="table">
    <tr>
        <th>Recruit ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Position</th>
       
    </tr>

    @foreach (var recruit in @Model)
    {
        <tr>
            <td><a href="/Recruit/ViewRecruit/@recruit.RecruitID">@recruit.RecruitID</a></td>
            <td>@recruit.FirstName</td>
            <td>@recruit.LastName</td>
            <td>@recruit.PositionName</td>
            

        </tr>
    }

</table>
